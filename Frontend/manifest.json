{
  "name": "Epilepsy Extension",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js":["content.js"]
    }
  ],
  "permissions": [
    "http://*/*", "https://*/*","http://127.0.0.1:5000/check",
    "tabs"
  ],
  "browser_action": {
      "default_popup" : "popup.html",
      "default_title": "Epilepsy Extension"
  }
}
